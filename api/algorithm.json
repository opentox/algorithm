{
    "swagger": "2.0",
    "info": {
        "version": "5.0.0pre1",
        "title": "OpenTox Algorithm service",
        "description": "An OpenTox REST Webservice. <br >Provides access to OpenTox algorithms.",
        "contact": {
            "name": "Christoph Helma, Andreas Maunz, in silico toxicology",
            "url": "http://www.in-silico.ch",
            "email": "support@in-silico.ch"
        },
        "license": {
            "name": "GNU GENERAL PUBLIC LICENSE",
            "url": "https://github.com/opentox/algorithm/blob/master/LICENSE"
        }
    },
    "host": "enm.in-silico.ch",
    "schemes": [
        "https"
    ],
    "paths": {
        "/algorithm": {
            "head": {
                "tags": [
                    "algorithm"
                ],
                "description": "for service check",
                "responses": {
                    "200": {
                        "$ref": "#/responses/200"
                    },
                    "400": {
                        "$ref": "#/responses/400"
                    },
                    "401": {
                        "$ref": "#/responses/401"
                    },
                    "404": {
                        "$ref": "#/responses/404"
                    }
                }
            },
            "get": {
                "tags": [
                    "algorithm"
                ],
                "description": "Get a list of all available algorithm",
                "produces": [
                    "text/uri-list"
                ],
                "responses": {
                    "200": {
                        "$ref": "#/responses/200"
                    },
                    "400": {
                        "$ref": "#/responses/400"
                    },
                    "401": {
                        "$ref": "#/responses/401"
                    },
                    "404": {
                        "$ref": "#/responses/404"
                    }
                }
            }
        },
